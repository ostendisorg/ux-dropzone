{
    "name": "@ostendis/ux-dropzone",
    "description": "File input dropzones for Symfony Forms",
    "license": "MIT",
    "version": "0.0.6",
    "main": "dist/controller.js",
    "types": "dist/controller.d.ts",
    "symfony": {
        "controllers": {
            "dropzone": {
                "main": "dist/controller.js",
                "webpackMode": "eager",
                "fetch": "eager",
                "enabled": true,
                "autoimport": {
                    "@symfony/ux-dropzone/src/style.css": true
                }
            }
        }
    },
    "peerDependencies": {
        "@hotwired/stimulus": "^3.0.0"
    },
    "devDependencies": {
        "@hotwired/stimulus": "^3.0.0"
    }
}
